<h3>Генератор рандом чисел</h3>
<script language="JavaScript">
function randomInteger(min, max) {
var rand = min + Math.random() * (max - min)
rand = Math.round(rand);
return rand;
}
umber = randomInteger(1, 6);
console.log(number);
if (number == 1) {
document.write("<h3>68</h3>")
}
if (number == 2) {
document.write("<h3>72</h3>")
}
if (number == 3) {
document.write("<h3>65</h3>")
}
if (number == 4) {
document.write("<h3>75</h3>")
}
if (number == 5) {
document.write("<h3>63</h3>")
}
if (number == 6) {
document.write("<h3>79</h3>")
</script>
